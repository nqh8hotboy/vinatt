{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "no-var": 1,
    "semi": [1, "never"],
    "quotes": [1, "single"],
    "new-cap": 0,
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "curly": [1, "multi-line"]
  }
}